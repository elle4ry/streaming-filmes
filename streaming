MERGE (d)-[:DIRECTED]->(m);
MATCH (d:Director {nome:'Christopher Nolan'}),(m:Movie {nome:'Oppenheimer'}),(s:Series {nome:'Peaky Blinders'})
MERGE (d)-[:DIRECTED]->(m) 
MERGE (d)-[:DIRECTED]->(s);
